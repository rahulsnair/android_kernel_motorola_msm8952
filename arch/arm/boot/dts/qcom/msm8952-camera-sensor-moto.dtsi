		qcom,cci-master = <0>;
		qcom,num-blocks = <33>;
		qcom,i2c-freq-mode = <1>;

		/* Stream ON */
		qcom,page0 = <1 0x0100 2 0x01 1 10>;
		qcom,poll0 = <1 0x0 2 0 1 1>;
		qcom,mem0 = <0 0x0 2 0 1 0>;

		/* Now read otp */
		qcom,page1 = <1 0x3d84 2 0xc0 1 5>;
		qcom,pageen1 = <1 0x3d81 2 0x01 1 10>;
		qcom,poll1 = <0 0x0 2 0 1 1>;
